{
  "version": "2",
  "plugins": [
    {
      "name": "rust-gen",
      "env": [
        "RUST_BACKTRACE",
        "RUST_LOG"
      ],
      "wasm": {
        "url": "file://./../../../target/wasm32-wasip1/release/sqlc-gen.wasm",
        "sha256": "b170682cec0aaccf31953790943c1ad2e2448d9fcaf1ad27ecd7c68c622ddade"
      }
    }
  ],
  "sql": [
    {
      "schema": "postgresql/schema",
      "queries": "postgresql/query",
      "engine": "postgresql",
      "codegen": [
        {
          "out": "postgresql",
          "plugin": "rust-gen",
          "options": {
            "use_async": true,
            "use_deadpool": false
          }
        }
      ]
    }
  ]
}
